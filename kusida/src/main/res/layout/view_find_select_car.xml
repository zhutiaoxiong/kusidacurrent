<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:androidme="http://schemas.android.com/apk/res-auto"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
    <view.view4me.set.ClipTitleMeSet
        android:id="@+id/title_head"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        androidme:text="@string/select_the_vehicle"/>

    <ListView
        android:id="@+id/view_find_selectcar_listview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title_head"
        android:layout_above="@+id/view_find_selectcat_submmit"/>

    <Button
        android:id="@+id/view_find_selectcat_submmit"
        style="@style/Button_round"
        android:layout_alignParentBottom="true"
        android:layout_margin="20dp"
        android:text="@string/confirm"/>


</RelativeLayout>
