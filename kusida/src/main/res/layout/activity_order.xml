<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:androidme="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <view.home.commonview.CommonTitletem
        android:id="@+id/titleme"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        androidme:text="命令下发"
        androidme:rightres="@drawable/page_order_xiafa_record"
      />
    <View
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/titleme"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:background="#ececec"/>
    <view.home.commonview.CommonMoreItem
        android:id="@+id/order_work_mode"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        androidme:text="工作模式"
        androidme:leftres="@drawable/page_order_work_mode"
        android:layout_marginTop="5dp"
        androidme:splitLine="false"
        />
    <view.home.commonview.CommonMoreItem
        android:id="@+id/order_vibration_sensitivity"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        androidme:text="震动灵敏度"
        androidme:leftres="@drawable/page_order_zhendong_lingmin"
        androidme:splitLine="false"
        />
    <view.home.commonview.CommonMoreItem
        android:id="@+id/order_vibration_alarm"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        androidme:text="震动报警"
        androidme:leftres="@drawable/page_order_zhendong"
        androidme:splitLine="false"
        />
    <view.home.commonview.CommonMoreItem
        android:id="@+id/order_speed_alarm"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        androidme:text="超速报警"
        androidme:leftres="@drawable/page_order_chaosu"
        androidme:splitLine="false"
        />
    <view.home.commonview.CommonMoreItem
        android:id="@+id/order_tamper_alarm"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        androidme:text="防拆报警"
        androidme:leftres="@drawable/page_order_fangchai"
        androidme:splitLine="false"
        />
    <view.home.commonview.CommonMoreItem
        android:id="@+id/order_auditory_function"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        androidme:text="听音功能"
        androidme:leftres="@drawable/page_order_tingyin"
        androidme:splitLine="false"
        android:visibility="gone"
        />
    <view.home.commonview.CommonMoreItem
        android:id="@+id/order_center_number"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        androidme:text="中心号码"
        androidme:leftres="@drawable/page_order_center_num"
        androidme:splitLine="false"
        android:visibility="gone"
        />
    <view.home.commonview.CommonMoreItem
        android:id="@+id/order_static_alarm"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        androidme:text="静止报警"
        androidme:leftres="@drawable/page_order_jingzhi"
        androidme:splitLine="false"
        android:visibility="gone"
        />
    <view.home.commonview.CommonMoreItem
        android:id="@+id/order_sim"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        androidme:text="SIM卡切换"
        androidme:leftres="@drawable/page_order_sim"
        androidme:splitLine="false"
        android:visibility="gone"
        />
</LinearLayout>