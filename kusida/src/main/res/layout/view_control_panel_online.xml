<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="ContentDescription,HardcodedText,SpUsage">


    <!--<TextView-->
    <!--android:id="@+id/txt_go_dressup"-->
    <!--android:layout_width="50dp"-->
    <!--android:layout_height="70dp"-->
    <!--android:layout_centerHorizontal="true"-->
    <!--android:layout_marginTop="60dp"-->
    <!--android:alpha="0.01"-->
    <!--android:background="@color/white"-->
    <!--android:enabled="false"-->
    <!--android:text=""-->
    <!--android:visibility="gone" />-->

    <ImageView
        android:id="@+id/img_refresh"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="45dp"
        android:padding="5dp"
        android:scaleType="fitXY"
        android:src="@drawable/btn_refresh_car"
        android:visibility="gone" />

    <ImageView
        android:id="@+id/iv_alibaba"
        android:layout_width="50dp"
        android:layout_height="55dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="10dp"
        android:padding="13dp"
        android:scaleType="centerCrop"
        android:src="@drawable/img_alibaba"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/img_temporary_borrow_car"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_above="@+id/img_refresh"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="8dp"
        android:padding="5dp"
        android:scaleType="centerCrop"
        android:src="@drawable/img_tempor_borrow_car_no"
        android:visibility="invisible" />

    <ImageView
        android:id="@+id/img_switch_air_conditioning"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="141dp"
        android:layout_marginEnd="10dp"
        android:padding="5dp"
        android:scaleType="fitXY"
        android:src="@drawable/btn_selector_air_condition"
        android:visibility="invisible" />


    <ImageView
        android:id="@+id/img_switch_bluetooth"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="74dp"
        android:layout_marginEnd="10dp"
        android:padding="5dp"
        android:scaleType="fitXY"
        android:src="@drawable/icon_control_model_switch"
        android:visibility="invisible" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="20dp">


        <ImageView
            android:id="@+id/img_meter"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/img_car_meter"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/img_battery"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_above="@+id/img_meter"
            android:layout_margin="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/battery_on"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/img_oil"
            android:layout_width="22.5dp"
            android:layout_height="24.5dp"
            android:layout_marginTop="58dp"
            android:layout_marginEnd="4dp"
            android:layout_toStartOf="@+id/txt_oil"
            android:scaleType="centerCrop"
            android:src="@drawable/img_car_oil"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/img_wifi"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_above="@+id/img_battery"
            android:layout_margin="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/wifi_off"
            android:visibility="invisible" />

        <ImageView
            android:id="@+id/img_gps"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_above="@+id/img_wifi"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/gps_off"
            android:visibility="invisible" />


        <ImageView
            android:id="@+id/img_temperature"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_above="@+id/img_gps"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/icon_temperature_off"
            android:visibility="gone" />

        <ImageView
            android:id="@+id/img_net_model"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_above="@+id/img_temperature"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginBottom="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/icon_control_model_net_off"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/txt_meter"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="16dp"
            android:gravity="center"
            android:text="行驶里程"
            android:textColor="@color/black"
            android:textSize="11sp"
            android:visibility="invisible"
            android:layout_marginBottom="102dp"
            />

        <TextView
            android:id="@+id/txt_meter_km"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_alignParentBottom="true"
            android:layout_toEndOf="@+id/txt_meter"
            android:gravity="center"
            android:text="km"
            android:textColor="#0066ff"
            android:layout_marginStart="3dp"
            android:textSize="14sp"
            android:layout_marginBottom="104dp"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/txt_battery"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_above="@+id/txt_meter"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_toEndOf="@+id/img_battery"
            android:text="电压"
            android:textColor="@color/white"
            android:textSize="12dp"
            android:visibility="invisible" />
        <TextView
            android:id="@+id/txt_oil_l"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_marginStart="16dp"
            android:layout_above="@+id/txt_meter"
            android:layout_marginBottom="8dp"
            android:gravity="center"
            android:text="剩余油量"
            android:textColor="@color/black"
            android:textSize="11sp"
            android:visibility="invisible" />
        <TextView
            android:id="@+id/txt_oil"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_toEndOf="@+id/txt_oil_l"
            android:layout_above="@+id/txt_meter"
            android:layout_marginBottom="10dp"
            android:layout_marginStart="3dp"
            android:text="36.5L"
            android:textColor="#0066ff"
            android:gravity="center"
            android:textSize="14sp"
            android:visibility="invisible" />



        <TextView
            android:id="@+id/txt_wifi"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_above="@+id/txt_battery"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:layout_toEndOf="@+id/img_battery"
            android:text="@string/online"
            android:textColor="@color/white"
            android:textSize="12dp"
            android:visibility="invisible" />


        <TextView
            android:id="@+id/txt_gps"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_above="@+id/img_wifi"
            android:layout_toEndOf="@+id/img_gps"
            android:text="@string/gps_location"
            android:textColor="@color/white"
            android:textSize="12dp"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/txt_temperature"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_above="@+id/txt_gps"
            android:layout_marginBottom="10dp"
            android:layout_toEndOf="@+id/img_temperature"
            android:text="@string/the_temperature"
            android:textColor="@color/white"
            android:textSize="12dp"
            android:visibility="gone" />

        <TextView
            android:id="@+id/txt_net_model"
            android:layout_width="wrap_content"
            android:layout_height="18dp"
            android:layout_above="@+id/txt_temperature"
            android:layout_marginBottom="10dp"
            android:layout_toEndOf="@+id/img_net_model"
            android:text="@string/model_net"
            android:textColor="@color/white"
            android:textSize="12dp"
            android:visibility="invisible" />
        <LinearLayout
            android:id="@+id/left_seek_layout"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="130dp"
            android:layout_marginStart="22dp"
            android:orientation="vertical"
            android:visibility="invisible"
            >

            <TextView
                android:id="@+id/txt_seek_left_close"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:text="CLOSE"
                android:textColor="#616161"
                android:textSize="11dp"
                />

            <view.view4control.ViewClipSeekBar
                android:id="@+id/seekbar_leftdoor"
                android:layout_width="24dp"
                android:layout_height="81dp"
                android:layout_gravity="center_horizontal"
                android:visibility="visible"
                android:layout_marginTop="7dp"/>
            <TextView
                android:id="@+id/txt_seek_left_open"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="OPEN"
                android:textColor="#616161"
                android:textSize="11dp"
                android:visibility="visible"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="7dp"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/right_seek_layout"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginBottom="130dp"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="22dp"
            android:orientation="vertical"
            android:visibility="invisible">

            <TextView
                android:id="@+id/txt_seek_right_close"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:gravity="center"
                android:text="CLOSE"
                android:textColor="#616161"
                android:textSize="11dp"
              />

            <view.view4control.ViewClipSeekBar
                android:id="@+id/seekbar_right_door"
                android:layout_width="24dp"
                android:layout_height="81dp"
                android:layout_gravity="center_horizontal"
                android:visibility="visible"
                android:layout_marginTop="7dp" />
            <TextView
                android:id="@+id/txt_seek_right_open"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="OPEN"
                android:textColor="#616161"
                android:textSize="11dp"
                android:visibility="visible"
                android:gravity="center"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="7dp"/>
        </LinearLayout>


    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/progress_layout"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerInParent="true"
        android:background="@color/black"
        android:gravity="top"
        android:visibility="invisible">

        <ProgressBar
            android:id="@+id/loading_view_progressbar"
            style="@style/loadingViewProgressBarStyle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:visibility="visible" />

        <TextView
            android:id="@+id/load_txt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/loading_view_progressbar"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="4dp"
            android:layout_marginBottom="20dp"
            android:text=""
            android:textColor="#ffffff"
            android:textSize="12dp" />
    </RelativeLayout>
</RelativeLayout>