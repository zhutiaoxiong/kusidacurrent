<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:androidme="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:ignore="HardcodedText,ContentDescription">
    <view.view4me.set.ClipTitleMeSet
        android:id="@+id/title_head"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        androidme:text="@string/problem_set"
        />

    <view.safetyneed.ViewAnswerSetItem
        android:id="@+id/question1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <view.safetyneed.ViewAnswerSetItem
        android:id="@+id/question2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"/>
    <view.safetyneed.ViewAnswerSetItem
        android:id="@+id/question3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"/>
    <Button
        android:id="@+id/view_answer_submmit"
        style="@style/Button_round"
        android:layout_margin="10dp"
        android:text="@string/confirm"/>


</LinearLayout>